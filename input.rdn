macro-env fooBar [foo, bar] {
  local (macro-env#HYG fbIntn = 0) {
    macro-env#ARG foo = f(val) macro-env#HYG fbIntn = val + 1;
    macro-env#ARG bar = f(valdec) {
      macro-env#HYG fbIntn = macro-env#HYG fbIntn - valdec;
      macro-env#HYG fbIntn;
    };
    f: macro-env#FUNC(macro-env#ARG foo, macro-env#ARG bar);
  };
};


lul = fooBar {
  print("foo. bar. is. fun.");
  foo 5;
  print(bar 4); // prints 2
};

lul();

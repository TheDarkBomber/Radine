macro-proc :for(iterator, condition, postaction, code) `{
  local loop(iterator) if condition {
    code
    loop(postaction);
  };
}';

for(`i = 0', `i < 10', `i + 1', `{
  for(`j = 0', `j < 10', `j + 1', `{
    write(i);
    write(", ");
    print(j);
  }');
}');
